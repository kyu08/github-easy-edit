{
  "name": "github-easy-edit",
  "description": "make editing pr description/comment on GitHub easier",
  "version": "0.1",
  "manifest_version": 3,
  "homepage_url": "https://github.com/kyu08/github-easy-edit",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "js": ["index.js"],
      "matches": ["https://github.com/*"],
      "css": ["index.css"]
    }
  ]
}
